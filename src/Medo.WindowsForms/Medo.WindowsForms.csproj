<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWindowsForms>true</UseWindowsForms>
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <AssemblyName>Medo.WinForms</AssemblyName>
    <RootNamespace>Medo.Windows.Forms</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <RunAnalyzersDuringBuild>true</RunAnalyzersDuringBuild>
    <EnableNETAnalyzers>true</EnableNETAnalyzers>
    <AnalysisLevel>latest</AnalysisLevel>
  </PropertyGroup>

  <PropertyGroup>
    <Description>Common classes</Description>
    <Authors>Josip Medved</Authors>
    <Copyright>Copyright (c) 2004 Josip Medved</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Medo\Medo.csproj" />
  </ItemGroup>

</Project>
